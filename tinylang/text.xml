<Program>
	<FunctionDecl Name="Sq" ReturnType="float">
		<Params>
			<Param Type="float">x</Param>
		</Params>
		<Block>
			<Return>
				<BinExpr Op="*">
					<Id>x</Id>
					<Id>x</Id>
				</BinExpr>
			</Return>
		</Block>
	</FunctionDecl>
	<FunctionDecl Name="XGreaterY" ReturnType="bool">
		<Params>
			<Param Type="float">x</Param>
			<Param Type="float">y</Param>
		</Params>
		<Block>
			<Decl>
				<Id Type="bool">ans</Id>
				<Bool>true</Bool>
			</Decl>
			<If>
				<Condition>
					<BinExpr Op=">">
						<Id>y</Id>
						<Id>x</Id>
					</BinExpr>
				</Condition>
				<IfTrue>
					<Block>
						<Assignment>
							<Id>ans</Id>
							<Bool>false</Bool>
						</Assignment>
					</Block>
				</IfTrue>
			</If>
			<Return>
				<Id>ans</Id>
			</Return>
		</Block>
	</FunctionDecl>
	<FunctionDecl Name="Increment" ReturnType="int">
		<Params>
			<Param Type="int">x</Param>
			<Param Type="int">y</Param>
		</Params>
		<Block>
			<ForLoop>
				<Decl>
					<Id Type="int">i</Id>
					<IntLiteral>0</IntLiteral>
				</Decl>
				<BinExpr Op="<">
					<Id>i</Id>
					<Id>y</Id>
				</BinExpr>
				<Assignment>
					<Id>i</Id>
					<BinExpr Op="+">
						<Id>i</Id>
						<IntLiteral>1</IntLiteral>
					</BinExpr>
				</Assignment>
				<Block>
					<Assignment>
						<Id>x</Id>
						<BinExpr Op="+">
							<Id>x</Id>
							<IntLiteral>1</IntLiteral>
						</BinExpr>
					</Assignment>
				</Block>
			</ForLoop>
			<Return>
				<Id>x</Id>
			</Return>
		</Block>
	</FunctionDecl>
	<FunctionDecl Name="Decrement" ReturnType="int">
		<Params>
			<Param Type="int">x</Param>
		</Params>
		<Block>
			<While>
				<Condition>
					<BinExpr Op=">">
						<Id>x</Id>
						<Bool>true</Bool>
					</BinExpr>
				</Condition>
				<Body>
					<Block>
						<Assignment>
							<Id>x</Id>
							<BinExpr Op="-">
								<Id>x</Id>
								<IntLiteral>1</IntLiteral>
							</BinExpr>
						</Assignment>
					</Block>
				</Body>
			</While>
			<Return>
				<Id>x</Id>
			</Return>
		</Block>
	</FunctionDecl>
	<Decl>
		<Id Type="float">x</Id>
		<FloatLiteral>2.5</FloatLiteral>
	</Decl>
	<Decl>
		<Id Type="float">y</Id>
		<FunctionCall Name="Sq">
			<Args>
				<Id>x</Id>
			</Args>
		</FunctionCall>
	</Decl>
	<Decl>
		<Id Type="int">z</Id>
		<FunctionCall Name="Increment">
			<Args>
				<IntLiteral>2</IntLiteral>
				<IntLiteral>10</IntLiteral>
			</Args>
		</FunctionCall>
	</Decl>
	<Decl>
		<Id Type="int">w</Id>
		<FunctionCall Name="Decrement">
			<Args>
				<IntLiteral>10</IntLiteral>
			</Args>
		</FunctionCall>
	</Decl>
	<Print>
		<Id>y</Id>
	</Print>
	<Print>
		<Id>z</Id>
	</Print>
	<Print>
		<Id>w</Id>
	</Print>
	<Print>
		<FunctionCall Name="XGreaterY">
			<Args>
				<Id>x</Id>
				<FloatLiteral>2.3</FloatLiteral>
			</Args>
		</FunctionCall>
	</Print>
</Program>
